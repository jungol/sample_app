<% if @feed_items.any? %>
  <ol class="microposts">
    <%= render partial: 'shared/feed_item', collection: @feed_items %>
  </ol>
  <%= will_paginate @feed_items %>
<% end %>

  
<script>
  $('.microposts li a').click(function() {
  	$micropost = $(this).parent().children('.content').text();
    mixpanel.track("Delete post",
    {
    	"micropost": $micropost
    });
  })
</script>